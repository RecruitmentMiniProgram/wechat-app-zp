.search_input {
  height: 90rpx;
  padding: 10rpx;
  background-color: #4a90e2;
}

.search_input navigator {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #fff;
  border-radius: 15rpx;
  color: #666;
}

@import "../../wxSearchView/wxSearchView.wxss";

.container {
  padding: 0;
  margin: 0;
}

.weui-search-bar,
.weui-search-bar__form,
.weui-search-bar__box,
.weui-search-bar__input {
  width: 100%;
}

.weui-search-bar {
  border: 0rpx;
  margin-top: 0rpx;
}